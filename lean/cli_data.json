{
  "modules": [
    {
      "type": [
        "brokerage",
        "cloud-brokerage",
        "data-queue-handler"
      ],
      "product-id": "181",
      "id": "Interactive Brokers",
      "installs": true,
      "configurations": [
       {
         "Id": "ib-agent-description",
         "Cloud-id": "accountType",
         "Type": "internal-input",
         "Value": "",
         "Environment": [
           "live","paper"
         ],
         "Value-options": [
           {
             "condition":{
                "type":"regex",
                "pattern":"^du|^df|^u",
                "dependent-config-id":"ib-account"
             },
             "value":"Individual"
           },
           {
             "condition":{
               "type":"regex",
               "pattern":"^di|^f|^i",
               "dependent-config-id":"ib-account"
             },
             "value":"Advisor"
           }
         ],
         "Input-method": "prompt",
         "Input-data":"Username", 
         "Help": "Your IB API key"
       },
       {
         "Id": "ib-trading-mode",
         "Cloud-id": "environment",
         "Type": "internal-input",
         "Value": "",
         "Environment": [
           "live","paper"
         ],
         "Value-options": [
           {
             "condition":{
                "type":"regex",
                "pattern":"^df|^du|^di",
                "dependent-config-id":"ib-account"
             },
             "value":"paper"
           },
           {
             "condition":{
               "type":"regex",
               "pattern":"^f|^i|^u",
               "dependent-config-id":"ib-account"
             },
             "value":"live"
           }
         ],
         "Input-method": "prompt",
         "Input-data":"Username", 
         "Help": "Your IB API key"
       },
       {
         "Id": "interactive-brokers-organization",
         "Type": "organization-id",
         "Value": "",
         "Environment": [
             "live",
             "paper"
         ],
         "Input-method": "prompt",
         "Input-data":"organization having subscription of module", 
         "Help": "The name or id of the organization with the Ronit module subscription",
         "Log-message": "Log messages are before outputting the prompt"
        },
       {
         "Id": "ib-user-name",
         "Cloud-id": "user",
         "Type": "input",
         "Value": "",
         "Environment": [
           "live",
           "paper"
         ],
         "Input-method": "prompt",
         "Input-data":"Username", 
         "Help": "Your IB API key"
       },
       {
         "Id": "ib-account",
         "Cloud-id": "account",
         "Type": "input",
         "Value": "",
         "Environment": [
           "live",
           "paper"
         ],
         "Input-method": "prompt",
         "Input-data":"Account id", 
         "Help": "Your IB API key"
       },
       {
         "Id": "ib-password",
         "Cloud-id": "password",
         "Type": "input",
         "Value": "",
         "Environment": [
           "live",
           "paper"
         ],
         "Input-method": "prompt-password",
         "Input-data":"ib-password", 
         "Help": "Your IB API key"
       },
       {
        "Id": "use-ib-feed",
        "Type": "input",
        "Value": "",
        "Environment": [
          "live",
          "paper", "cloud-brokerage"
        ],
        "Input-method": "confirm",
        "Input-data":"Do you want to use the Interactive Brokers price data feed instead of the QuantConnect price data feed?", 
        "Help": "Your IB API key"
      },
       {
          "Id": "environments",
          "Type": "configurations-env",
          "Value": [
            {
              "Name": "lean-cli",
              "Value": [
                {
                  "Name": "live-mode-brokerage",
                  "Value": "InteractiveBrokersBrokerage"
                },
                {
                  "Name": "transaction-handler",
                  "Value": "QuantConnect.Lean.Engine.TransactionHandlers.BrokerageTransactionHandler"
                },
                {
                 "Name": "data-queue-handler",
                 "Value": "QuantConnect.Brokerages.InteractiveBrokers.InteractiveBrokersBrokerage"
               },
               {
                 "Name": "history-provider",
                 "Value": "BrokerageHistoryProvider"
               }
              ]
            }
          ],
          "Environment": [
           "live", "paper"
         ]
        }
      ]
    },
    {
      "type": [
        "data-provider"
      ],
      "product-id": "",
      "id": "QuantConnect",
      "installs": false,
      "configurations": [
        {
          "Id": "quantconnect-organization",
          "Type": "input",
          "Value": "",
          "Environment": [
              "live",
              "paper"
          ],
          "Input-method": "prompt",
          "Input-data":"organization having subscription of module", 
          "Help": "The name or id of the organization with the Ronit module subscription"
         },
        {
          "Id": "data-provider",
          "Type": "info",
          "Value": "QuantConnect.Lean.Engine.DataFeeds.ApiDataProvider",
          "Environment": [
            "live"
          ]
        },
        {
          "Id": "map-file-provider",
          "Type": "info",
          "Value": "QuantConnect.Data.Auxiliary.LocalZipMapFileProvider",
          "Environment": [
            "live"
          ]
        },
        {
          "Id": "factor-file-provider",
          "Type": "info",
          "Value": "QuantConnect.Data.Auxiliary.LocalZipFactorFileProvider",
          "Environment": [
            "live"
          ]
        }
      ]
    },
    {
      "type": [
        "data-queue-handler"
      ],
      "product-id": "",
      "id": "IQFeed",
      "installs": false,
      "configurations": [
       {
         "Id": "iqfeed-iqconnect",
         "Type": "input",
         "Value": "",
         "Environment": [
          "live", "paper"
        ],
         "Input-method": "path-parameter",
         "Input-default":"C:/Program Files (x86)/DTN/IQFeed/iqconnect.exe",
         "Input-data":"IQConnect binary location", 
         "Help": "Your IB API key"
       },
       {
        "Id": "iqfeed-username",
        "Type": "input",
        "Value": "",
        "Environment": [
         "live", "paper"
       ],
        "Input-method": "prompt",
        "Input-data":"Username", 
        "Help": "Your IB API key"
      },
       {
        "Id": "iqfeed-password",
        "Type": "input",
        "Value": "",
        "Environment": [
          "live", "paper"
        ],
        "Input-method": "prompt-password",
        "Input-data":"Password", 
        "Help": "Your IB API key"
      },
      {
        "Id": "iqfeed-productName",
        "Type": "input",
        "Value": "",
        "Environment": [
         "live", "paper"
       ],
        "Input-method": "prompt",
        "Input-data":"Product id", 
        "Help": "Your IB API key"
      },
      {
        "Id": "iqfeed-version",
        "Type": "input",
        "Value": "",
        "Environment": [
          "live", "paper"
        ],
        "Input-method": "prompt",
        "Input-data":"Product version", 
        "Help": "Your IB API key"
      },
       {
          "Id": "environments",
          "Type": "configurations-env",
          "Value": [
            {
              "Name": "lean-cli",
              "Value": [
                {
                 "Name": "data-queue-handler",
                 "Value": "QuantConnect.ToolBox.IQFeed.IQFeedDataQueueHandler"
               },
               {
                "Name": "history-provider",
                "Value": "QuantConnect.ToolBox.IQFeed.IQFeedDataQueueHandler"
              }
              ]
            }
          ],
          "Environment": [
           "live", "paper"
         ]
        }
      ]
    }
  ]
}